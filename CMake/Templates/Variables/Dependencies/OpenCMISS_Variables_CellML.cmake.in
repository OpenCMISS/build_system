#[=======================================================================[.rst:
OpenCMISS-BuildSystem CellML Variables
--------------------------------------

Setup the variables to use CellML with OpenCMISS.

#]=======================================================================]

set(OpenCMISS_FIND_SYSTEM_CellML OFF CACHE BOOL "Find a system version of CellML rather than an OpenCMISS version." FORCE)

# CellML repository options
set(OpenCMISS_CellML_VERSION 3.11.0 CACHE STRING "The version/tag of CellML to use." FORCE)
set(OpenCMISS_CellML_GIT_HOST @OC_DEPENDENCIES_GIT_HOST@ CACHE STRING "The hostname of the CellML Git repository." FORCE)
set(OpenCMISS_CellML_GIT_ORGANISATION @OC_DEPENDENCIES_GIT_ORGANISATION@ CACHE STRING "The organisation of the CellML Git repository." FORCE)
set(OpenCMISS_CellML_GIT_BRANCH @OC_DEPENDENCIES_GIT_BRANCH@ CACHE STRING "The branch of the CellML Git repository to use." FORCE)

# CellML configuration options
